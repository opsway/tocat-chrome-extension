{
  "manifest_version": 2,
  "key": "",
  "name":"TOCAT",
  "version":"1.0.3",
  "description":"TOCAT plugin",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "TOCAT",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://tocat.opsway.com/*",
    "https://accounts.google.com/*",
    "https://*.googleusercontent.com/*",
    "*://*.googleapis.com/*",
    "tabs",
    "identity"
  ],
  "background": {
    "scripts": ["build/js/background-assets.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://people.zoho.com/hr*", "https://people.zoho.eu/hr*"],
      "include_globs": ["*#attendance/report/hoursreport"],
      "css": ["build/css/assets.css", "build/css/content/people-zoho-com.css"],
      "js": ["build/js/libs.js", "build/js/content/people-zoho-com.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "build/css/assets.css",
    "build/js/libs.js",
    "fonts/*.*"
  ],
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "offline_enabled": false,
  "oauth2": {
    "client_id": "641843524050-oic7glk3tu44o8nvlngulfmtrtv62rqu.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email"
    ]
  }
}
