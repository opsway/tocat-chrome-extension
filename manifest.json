{
  "manifest_version": 2,
  "name":"TOCAT",
  "version":"0.1",
  "description":"TOCAT plugin",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "TOCAT",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://maxcdn.bootstrapcdn.com/bootstrap/*",
    "storage",
    "http://test.tocat.opsway.com/*",
    "tabs"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  }
}
