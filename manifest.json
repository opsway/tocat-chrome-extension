{
  "manifest_version": 2,
  "name":"TOCAT",
  "version":"0.6",
  "description":"TOCAT plugin",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "TOCAT",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "https://tocat.opsway.com/*",
    "tabs"
  ],
  "background": {
    "scripts": ["js/background.js", "js/tools.js"],
    "persistent": false
  }
}
